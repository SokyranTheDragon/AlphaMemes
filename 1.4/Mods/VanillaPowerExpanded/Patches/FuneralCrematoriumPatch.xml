			<li>Vanilla Furniture Expanded - Power</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RitualObligationTargetFilterDef[defName="AM_FuneralTargetCremate"]/thingDefs</xpath>
					<value>
						<li>VPE_GasCrematorium</li>
					</value>
				</li>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/RitualObligationTargetFilterDef[defName="AM_FuneralTargetCremateUrnStuff"]/thingDefs</xpath>
					<value>
						<li>VPE_GasCrematorium</li>
					</value>
				</li>				
			</operations>
		</match>
	</Operation>
</Patch>

